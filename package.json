{
  "name": "tx2uml",
  "version": "1.1.0",
  "description": "Ethereum transaction visualizer that generates UML sequence diagrams.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc --build ./tsconfig.json",
    "clean": "tsc --build --clean ./tsconfig.json",
    "examples": "java -jar ./lib/plantuml.jar ./examples/syntax.puml ./examples/delegate.puml",
    "prettier:check": "prettier --check --no-semi '**/*.{ts,md}'",
    "prettier:fix": "prettier --write --no-semi '**/*.{ts,md}'",
    "test": "npx jest"
  },
  "author": "Nick Addison",
  "repository": "github:naddison36/tx2uml",
  "license": "MIT",
  "files": [
    "lib/*.js",
    "lib/*.d.ts",
    "lib/clients/*.js",
    "lib/clients/*.d.ts",
    "lib/plantuml.jar",
    "lib/utils/*.js",
    "lib/utils/*.d.ts"
  ],
  "bin": {
    "tx2uml": "lib/tx2uml.js"
  },
  "keywords": [
    "Unified Modeling Language",
    "UML",
    "Ethereum",
    "Sequence diagram",
    "Sequence",
    "diagram",
    "transaction",
    "trace",
    "debug",
    "Blockchain",
    "visual",
    "transfer",
    "security",
    "audit"
  ],
  "dependencies": {
    "axios": "^0.27.2",
    "axios-debug-log": "^0.8.4",
    "bignumber.js": "^9.0.2",
    "commander": "^9.4.0",
    "debug": "^4.3.4",
    "ethers": "^5.6.9",
    "p-limit": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.4",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
