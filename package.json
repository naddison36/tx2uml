{
  "name": "tx2uml",
  "version": "1.0.19",
  "description": "Ethereum transaction visualizer that generates UML a sequence diagram from transaction contract calls.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc --build ./tsconfig.json",
    "clean": "tsc --build --clean ./tsconfig.json",
    "examples": "java -jar ./lib/plantuml.jar ./examples/syntax.puml ./examples/delegate.puml",
    "prettier:check": "prettier --check --no-semi '**/*.{ts,md}'",
    "prettier:fix": "prettier --write --no-semi '**/*.{ts,md}'",
    "test": "npx jest"
  },
  "author": "Nick Addison",
  "repository": "github:naddison36/tx2uml",
  "license": "MIT",
  "files": [
    "lib/*.js",
    "lib/*.d.ts",
    "lib/clients/*.js",
    "lib/clients/*.d.ts",
    "lib/plantuml.jar",
    "lib/utils/*.js",
    "lib/utils/*.d.ts"
  ],
  "bin": {
    "tx2uml": "lib/tx2uml.js"
  },
  "keywords": [
    "Unified Modeling Language",
    "UML",
    "Ethereum",
    "Sequence diagram",
    "Sequence",
    "diagram",
    "transaction",
    "trace",
    "debug",
    "Blockchain",
    "visual"
  ],
  "dependencies": {
    "@ethersproject/abstract-provider": "^5.6.1",
    "@ethersproject/providers": "^5.6.8",
    "axios": "^0.27.2",
    "axios-debug-log": "^0.8.4",
    "bignumber.js": "^9.0.1",
    "commander": "^9.2.0",
    "debug": "^4.3.4",
    "ethers": "^5.6.8",
    "p-limit": "3.1.0",
    "verror": "^1.10.1"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^16.11.36",
    "@types/verror": "^1.10.5",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.8.0",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  }
}
